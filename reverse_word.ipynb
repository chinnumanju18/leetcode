      "source": [
        "class Solution:\n",
        "    def reverseWords(self, s):\n",
        "        words = s.split()\n",
        "        reversed_words = words[::-1]\n",
        "        return ' '.join(reversed_words)\n",
        "s = \"  the sky is  blue  \"\n",
        "solution = Solution()\n",
        "print(solution.reverseWords(s))\n"
      ]
    }
  ]
